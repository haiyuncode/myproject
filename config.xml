<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="test.compan.project" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Test</name>
  <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>

  <allow-navigation href="cdn.contentful.com/*" />
  <allow-navigation href="*://*.cdn.contentful.com/*" />
  <allow-navigation href="http://cdn.contentful.com/spaces/wiydhpfvx7qb/entries/3NB130JtnWOOoogi6s4w6q?access_token=3e89ff15f153f0bf8a1b97818f32c5883f7f582a8be329fdbefde21ea375aaa7" />
  <allow-navigation href="http://*/*" />
  <allow-navigation href="*" />

  <content src="index.html"/>
  <access origin="content:///*"/>
  <access origin="*"/>

  <allow-navigation href="http://*/*" />
  <allow-navigation href="https://*/*" />
  <allow-navigation href="data:*" />

  <!-- Allow images, xhrs, etc. to google.com -->
  <access origin="http://cdn.contentful.com" />
  <access origin="https://cdn.contentful.com" />

  <allow-intent href="https://cdn.contentful.com*" />
  <allow-intent href="http://cdn.contentful.com*" />
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>


  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <engine name="ios" spec="~3.9.2"/>
  <engine name="android" spec="~4.1.1"/>
</widget>
